<div class="row" style="width:100%;">
    <form class="col s12 m12">
        <div class="row">
            <div class="input-field col s12 m6 offset-m3">
                <input placeholder="{{placeholder}}" id="onePassword" type="text" onkeyup="return forceLower(this);"
                    style="color:white;">
                <label for="onePassword">{{fieldLabel}}</label>
                <p style="color:white;" id="responseText"> </p>
            </div>
            <div class="input-field col s3">
                <a onclick="submitStep();" class="waves-effect waves-light btn grey lighten-5"
                    style="color:black;">Submit</a>
            </div>
    </form>
    <input type="hidden" id="login" name="login" value="{{login}}">
</div>